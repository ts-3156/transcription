{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "$": true,
    "Snackbar": true,
    "JQuery": true
  },
  "rules": {
    "semi": "error",
    "@typescript-eslint/no-this-alias": "warn",
    "@typescript-eslint/camelcase": "warn"
  }
}
