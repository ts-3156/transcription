{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "$": true,
    "Snackbar": true,
    "JQuery": true
  },
  "rules": {
    "semi": "error"
  }
}
